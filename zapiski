gcc begining.c -o program

primer ot zada4a:
prosta:
gcc -c bitmask.c -o bitmask.o
ar -rcs libsprogram.a bitmask.o
gcc bitmask.c -o sprogram -L. -l sprogram
./sprogram
dinami4na:
 gcc -c -fPIC bitmask.c -o bitmask.o
 gcc -shared -o libdprogram.so bitmask.o
 gcc bitmask.c -o dprogram -L. -l dprogram
 echo $pwd

dinami4na biblioteka

